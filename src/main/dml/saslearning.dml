class ElementFragment {
	String name;
	boolean linked;
}



relation documentHasSrcFragments {
	Document playsRole document {
		multiplicity 0..1;
	}
	
	ElementFragment playsRole fragment {
		multiplicity 0..*;
	}
}

relation fragmentHasChild {
	ElementFragment playsRole parent {
		multiplicity 0..1;
	}
	
	ElementFragment playsRole child {
		multiplicity 0..1;
	}
}

relation fragmentHasAnnotation {
	ElementFragment playsRole fragment {
		multiplicity 0..1;
	}
	
	Annotation playsRole annotation {
		multiplicity 0..1;
	}	
}

class ScenarioFragment extends ElementFragment{}
class SrcOfStimulusFragment extends ElementFragment{}
class StimulusFragment extends ElementFragment{}
class ArtifactFragment extends ElementFragment{}
class EnvironmentFragment extends ElementFragment{}
class ResponseFragment extends ElementFragment{}
class ResponseMeasureFragment extends ElementFragment{}
